*cc1:
%{!mandroid|tno-android-cc:%{profile:-p} %{fsanitize=*:-funwind-tables};:%{profile:-p} %{fsanitize=*:-funwind-tables} %{!mglibc:%{!muclibc:%{!mbionic: -mbionic}}} %{!fno-pic:%{!fno-PIC:%{!fpic:%{!fPIC: -fPIC}}}}}

